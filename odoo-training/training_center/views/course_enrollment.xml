<odoo>
    <record id="action_training_center_course_enrollment" model="ir.actions.act_window">
		<field name="name">Course Enrollment</field>
		<field name="res_model">training_center.course_enrollment</field>
		<field name="view_mode">tree,form</field>
	</record>
    
    <menuitem id="menu_course_enrollment" name="Enrollment" parent="menu_training1"  action="action_training_center_course_enrollment"/>
    
    <record id="course_enrollment_tree_view"  model="ir.ui.view">
        <field name="name">Course Enrollment Tree</field>
        <field name="model">training_center.course_enrollment</field>
        <field name="arch" type="xml">
        <tree>
            <field name="title"/>
            <field name="start_date"/>
        </tree>
        </field>

    </record>
</odoo>